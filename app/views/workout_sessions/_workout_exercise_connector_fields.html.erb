<li>
  <%= f.fields_for :exercise_attributes do |exercise| %>
    <%= exercise.label :name, "Exercise" %>
    <%= exercise.text_field :name, required: true %>
  <% end %>

  <%= f.label :reps %>
  <%= f.text_field :reps, required: true %>
  <%= f.label :sets %>
  <%= f.text_field :sets, required: true %>
  <%= f.label :rest_time %>
  <%= f.text_field :rest_time, required: true %>


  <%= link_to_remove_association "Remove Exercise", f %>
</li>